div>\n              <IconButton \n                variant='contained' className='category_btn'\n                style={{color: 'green'}}\n                onClick={()=>{addToCart()}}>                     \n              <ShoppingCart  fontSize='large'/>\n                BUY\n            </IconButton>\n            { addedNumber() }\n            </Paper>                \n          </Grid>                       \n        </Grid>\n        <Grid container >        \n            <Grid item xs={12} sm={2} md={12} lg={2} ></Grid>\n            <Grid item xs={12} sm={8} md={8} lg={8}><PromotionStepper/></Grid> \n        </Grid> \n      </div>\n  )\n\n  function addToCart(){\n    let currentCart = JSON.parse(JSON.stringify(cart))\n    if(currentCart[product.id]){\n      currentCart[product.id].count++\n    }\n    else{\n      currentCart[product.id] = { \n        'price': product.price, \n        'count': 1,\n        'name': product.name,\n        'id': product.id\n      }\n    }\n    cookie.save('cart', currentCart, { path: '/' })\n    setCart(currentCart)\n  }\n\n  function addedNumber() {\n    if(cart[product.id]){\n      return `Added to cart ${ cart[product.id].count > 1 ? `x${cart[product.id].count}` : '' }`\n    } else {\n      return '';\n    }\n  }\n}","/home/luba/projects/shop_front/src/components/header.js",["85"],"/home/luba/projects/shop_front/src/components/order.js",["86","87"],"/home/luba/projects/shop_front/src/services/orderApi.js",[],"/home/luba/projects/shop_front/src/components/productsList.js",["88"],{"ruleId":"89","severity":1,"message":"90","line":8,"column":10,"nodeType":"91","messageId":"92","endLine":8,"endColumn":18},{"ruleId":"89","severity":1,"message":"93","line":9,"column":10,"nodeType":"91","messageId":"92","endLine":9,"endColumn":15},{"ruleId":"89","severity":1,"message":"94","line":10,"column":10,"nodeType":"91","messageId":"92","endLine":10,"endColumn":20},{"ruleId":"89","severity":1,"message":"95","line":10,"column":22,"nodeType":"91","messa